{
    "plugins": ["jest"],
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off"/* ,
        "quotes": [
            "error",
            "double"
        ]     */
    }
}
